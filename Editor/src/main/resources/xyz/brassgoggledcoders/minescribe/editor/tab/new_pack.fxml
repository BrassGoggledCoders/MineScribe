<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="xyz.brassgoggledcoders.minescribe.editor.controller.tab.NewPackController">
    <VBox AnchorPane.topAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0">
        <AnchorPane fx:id="formContainer">

        </AnchorPane>
        <HBox alignment="CENTER_RIGHT">
            <padding>
                <Insets bottom="10" left="20" right="20"/>
            </padding>
            <Button fx:id="submit" onMouseClicked="#onSubmit" text="Create New Pack"/>
        </HBox>
    </VBox>
</AnchorPane>
